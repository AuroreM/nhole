{
  "name": "Client",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "properties": {
    "id": {
      "type": "bigserial",
      "required": true
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "number": {
      "type": "string",
      "required": true
    },
    "morning": {
      "type": "boolean",
      "required": true
    },
    "afternoon": {
      "type": "boolean",
      "required": true
    },
    "lunch": {
      "type": "boolean",
      "required": true
    },
    "evening": {
      "type": "boolean",
      "required": true
    },
    "userId": {
      "type": "bigserial",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "messages": {
      "type": "hasMany",
      "model": "Message",
      "foreignKey": "clientId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": ["find"]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
